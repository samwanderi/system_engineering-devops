#!/usr/bin/env bash
# This script loops from 1 to 10 and prints "bad luck" for 4, "good luck" for 8, and "Best School" otherwise

count=1
while [ $count -le 10 ]
do
    if [ $count -eq 4 ]; then
        echo "bad luck"
    elif [ $count -eq 8 ]; then
        echo "good luck"
    else
        echo "Best School"
    fi
    ((count++))
done
